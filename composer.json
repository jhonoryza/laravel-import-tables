{
    "name": "jhonoryza/laravel-import-tables",
    "description": "laravel package handling import tables",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jhonoryza\\LaravelImportTables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jhonoryza\\LaravelImportTables\\ServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "fajar sidik priatna",
            "email": "jardik.oryza@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0|^10.0",
        "pestphp/pest": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
